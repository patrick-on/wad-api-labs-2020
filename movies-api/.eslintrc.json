{
    "env": {
        "browser": true,
        "es2021": true
    },
    "globals": {
        "process": true
      },
    
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "semi": 1,
        "no-console": "off"
    }
}
